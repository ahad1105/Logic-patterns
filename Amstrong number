public class Main
{
    public static void main(String args[])
    {
        int n = 155;
        int temp = n;  //for doing some operations on n, so that we dont use the original value. 
        int r,sum = 0;  //r is use for isolating the value of int variable. 
        
        while(n>0)  //run it 3 times
        {
            r = n%10;
            n = n/10;
            sum = sum + r*r*r;
            
        }
        if(temp == sum)
        {
            System.out.println("Its a amstrong number");
        }
        else
        {
            System.out.println("Not a amstrong number");  
        }
    }
}
