public class main
{
    public static void main(String args[])
    {
        int i;
        int j;
        for(i=0;i<4;i++)
        {
            for(j=0;j<4;j++)  //for rows
            {
                System.out.print("*");
            }
        System.out.println();  //for columns 
        }
    }
}

output - 
****
****
****
****
