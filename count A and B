public class String
{
    public static void main(String args[])
    {
        int count1 = 0;
        int count2 = 0;
        String str = "ABUBUBABUBUB";
        char c = str.charAt(i);
        for(int i=0;i<=str.length();i++)
        {
            if(c==A)
            {
                count1++;
            }
            elif(c==B);
            {
                count2++;
            }
        System.out.println(count1);
        System.out.println(count2);
        }
    }
}
