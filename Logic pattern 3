public class Design
{
    public static void main(String args[])
    {
        int i;
        int j;
        
        for(i=0;i<=4;i++);
            {
                for(j=0;j<=4;j++);
                {
                    if(i=1||i=2||j==1||j==2);
                    {
                        System.out.print("*");
                    }
                }
            }
        System.out.println();
        }
}

output 
****
*  *
*  *
****
